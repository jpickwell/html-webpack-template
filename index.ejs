<% var item, key %>
<% var files = htmlWebpackPlugin.files %>
<% var options = htmlWebpackPlugin.options %>
<% var manifest = files.manifest %>
<% var meta = options.meta || {} %>
<% var title = options.title || 'Webpack App' %>
<% var favicon = files.favicon %>
<% var links = options.links || [] %>
<% var css = files.css %>
<% var baseHref = options.baseHref %>
<% var appMountElement = options.appMountElement || 'div' %>
<% var appMountId = options.appMountId %>
<% var appMountIds = options.appMountIds || [] %>
<% var globals = options.window %>
<% var scripts = options.scripts || [] %>
<% var chunks = files.chunks %>
<% var devServer = options.devServer %>
<% var googleAnalytics = options.googleAnalytics %>
<% var trackingId = googleAnalytics && googleAnalytics.trackingId %>
<% var modernizr = options.modernizr %>
<% var outdatedBrowser = options.outdatedBrowser %>
<% var browserUpdate = options.browserUpdate %>

<!DOCTYPE html>
<% if (modernizr) { %>
<html class="no-js" lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>>
<% } else { %>
<!--[if lt IE 7 ]>             <html class="ie6" lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>> <![endif]-->
<!--[if IE 7 ]>                <html class="ie7" lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>> <![endif]-->
<!--[if IE 8 ]>                <html class="ie8" lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>> <![endif]-->
<!--[if IE 9 ]>                <html class="ie9" lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en"<% if (manifest) { %> manifest="<%= manifest %>"<% } %>> <!--<![endif]-->
<% } %>
  <head>
    <meta charset="utf-8">
    <meta content="ie=edge" http-equiv="x-ua-compatible">

    <title><%= title %></title>

    <% for (key in meta) { %>
    <meta content="<%= meta[key] %>" name="<%= key %>">
    <% } %>

    <% if (options.mobile) { %>
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <% } %>

    <% if (favicon) { %>
    <link href="<%= favicon %>" rel="shortcut icon">
    <% } %>

    <% if (outdatedBrowser) { %>
    <link crossorigin="anonymous" href="https://cdnjs.cloudflare.com/ajax/libs/outdated-browser/1.1.3/outdatedbrowser.min.css" integrity="sha256-KNfTksp/+PcmJJ0owdo8yBLi/SVMQrH/PNPm25nR/pI=" rel="stylesheet">
    <% } %>

    <% for (item of links) { %>
    <% if (typeof item === 'string' || item instanceof String) { item = { href: item, rel: 'stylesheet' } } %>
  	<link<% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>>
    <% } %>

    <% for (key in css) { %>
    <link href="<%= css[key] %>" rel="stylesheet">
    <% } %>

    <% if (baseHref) { %>
    <base href="<%= baseHref %>">
    <% } %>

    <% if (modernizr) { %>
    <script crossorigin="anonymous" integrity="sha256-0rguYS0qgS6L4qVzANq4kjxPLtvnp5nn2nB5G1lWRv4=" src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
    <% } %>
  </head>
  <body>
    <% if (options.unsupportedBrowser) { %>
    <style>.unsupported-browser { display: none; }</style>
    <div class="unsupported-browser">
      Sorry, your browser is not supported. Please upgrade to the latest version or switch your browser to use this
      site. See <a href="http://outdatedbrowser.com/">outdatedbrowser.com</a> for options.
    </div>
    <% } %>

    <% if (appMountId) { appMountIds.unshift(appMountId) } %>
    <% for (item of appMountIds) { %>
    <% if (typeof item === 'string' || item instanceof String) { item = { id: item } } %>
    <<%= appMountElement %><% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>></<%= appMountElement %>>
    <% } %>

    <% if (outdatedBrowser) { %>
    <div id="outdated">
      <h6>Your browser is out-of-date!</h6>
      <p>
        Update your browser to view this website correctly.
        <a href="http://outdatedbrowser.com/" id="btnUpdateBrowser">Update my browser now.</a>
      </p>
      <p class="last">
        <a href="#" id="btnCloseUpdateBrowser" title="Close">&times;</a>
      </p>
    </div>

    <script crossorigin="anonymous" integrity="sha256-yV0saZESxHBqfSfNncH044y3GHbsxLZJbQQmuxrXv90=" src="https://cdnjs.cloudflare.com/ajax/libs/outdated-browser/1.1.3/outdatedbrowser.min.js" type="text/javascript"></script>

    <script type="text/javascript">
      !function(o,n){var f=o.onload
      o.onload="function"!=typeof o.onload?n:function(){f&&f(),n()}}(window,function(){outdatedBrowser({bgColor:"#f25648",color:"#ffffff",lowerThan:"transform",languagePath:""})})
    </script>
    <% } %>

    <% if (browserUpdate) { %>
    <script type="text/javascript">
      function $buo_f(){var e=document.createElement("script")
      e.src="//browser-update.org/update.min.js",document.body.appendChild(e)}var $buoop={vs:{i:10,f:-4,o:-4,s:7,c:-4},unsecure:!0,c:4}
      try{document.addEventListener("DOMContentLoaded",$buo_f,!1)}catch(e){window.attachEvent("onload",$buo_f)}
    </script>
    <% } %>

    <% if (globals) { %>
    <script type="text/javascript">
      <% for (key in globals) { %>
      window['<%= key %>'] = <%= JSON.stringify(globals[key]) %>;
      <% } %>
    </script>
    <% } %>

    <% for (item of scripts) { %>
    <% if (typeof item === 'string' || item instanceof String) { item = { src: item, type: 'text/javascript' } } %>
    <script<% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>></script>
    <% } %>

    <% for (key in chunks) { %>
    <script src="<%= chunks[key].entry %>" type="text/javascript"></script>
    <% } %>

    <% if (devServer) { %>
    <script src="<%= devServer %>/webpack-dev-server.js" type="text/javascript"></script>
    <% } %>

    <% if (googleAnalytics) { %>
    <script type="text/javascript">
      !function(o,n,f,a,t,e,r){o.GoogleAnalyticsObject=t,o[t]=o[t]||function(){(o[t].q=o[t].q||[]).push(arguments)},o[t].l=1*new Date,e=n.createElement(f),r=n.getElementsByTagName(f)[0],e.async=1,e.src=a,r.parentNode.insertBefore(e,r)}(window,document,"script","//www.google-analytics.com/analytics.js","ga")
      <% if (trackingId) { %>
      ga("create","<%= trackingId %>","auto")
      <% } else { throw new Error("html-webpack-template requires googleAnalytics.trackingId config"); }%>
      <% if (googleAnalytics.pageViewOnLoad) { %>
      ga("send","pageview")
      <% } %>
    </script>
    <% } %>
  </body>
</html>
